<!--pages/report/report.wxml-->
<view>
    <navigator class="nav-list" wx:key="index" wx:for="{{navList}}" url="{{item.url}}" open-type="navigate">
        <image class="logo" src="{{item.icon}}" />
        <view class="name">
            <text>{{item.name}}</text>
        </view>
        <image class="icon" src="/images/icon/icon-right.png" />
    </navigator>
</view>
